      "sourcesContent": [
        "import Hapi from '@hapi/hapi';\nimport { server } from 'karma';\n\nconst start = async() => {\n    const server = Hapi.server({\n        port:8000,\n        host:'localhost',\n    });\n\n    server.route({\n        method: 'GET',\n        path: '/login',\n        handler: (req,h) => {\n            return 'hello';\n        }\n    });\n    \n    await server.start();\n\n    console.log(`Server is listening on ${server.info.uri}`);\n}\n\nprocess.on('unhandledRejection', err => {\n    console.log(err);\n    process.exit(1); \n});\n\nstart();"
